    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2",
    "swagger-typescript-api": "^13.0.23",
    "typescript": "^5.7.3",
    "vite-plugin-dts": "^4.5.0",
    "zod": "^3.24.1"
